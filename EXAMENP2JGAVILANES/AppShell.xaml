<Shell
    x:Class="EXAMENP2JGAVILANES.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EXAMENP2JGAVILANES"
    Shell.FlyoutBehavior="Flyout"
    Title="EXAMENP2JGAVILANES">

    <FlyoutItem Title="Recargas" Icon="recargas.png">
        <ShellContent
            Title="Recargas"
            ContentTemplate="{DataTemplate local:RecargaPage}"
            Route="RecargaPage" />
    </FlyoutItem>

    <FlyoutItem Title="Grid" Icon="grid.png">
        <ShellContent
            Title="Grid"
            ContentTemplate="{DataTemplate local:GridJOHAO}"
            Route="GridJOHAO" />
    </FlyoutItem>

</Shell>
